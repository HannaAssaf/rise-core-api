generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum SupplierCode {
  farnell
  newark
  element14
  mock
}

model Product {
  id          String       @id @default(uuid())
  supplier    SupplierCode
  supplierSku String
  name        String

  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt

  @@unique([supplier, supplierSku], name: "uniq_supplier_sku")
  @@index([supplier])
  @@index([supplierSku])
}



